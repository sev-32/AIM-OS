agent:
  id: produce.insight_synthesizer
  name: Insight Synthesizer
  stage: produce
  stage_label: Produce
  role: analyst
  description: Executes Insight Synthesizer responsibilities within the Produce stage.
  capabilities: []
  policies:
  - policy.evidence_threshold
  - policy.latency_budget
  - policy.research_depth
  - policy.seed.evidence_threshold
  - policy.seed.max_research_depth
  - policy.seed.max_total_time
  depends_on:
  - produce.coordinator
  - orchestration.supervisor
  - orchestration.observability_hub
  - orchestration.policy_enforcer
  - orchestration.audit_trail
  - orchestration.retry_manager
  - orchestration.quality_board
  inputs:
  - produce.input
  outputs:
  - produce.output
  prompts:
  - prompts/produce/insight_synthesizer_primary.md
  - prompts/produce/insight_synthesizer_fallback.md
  - prompts/produce/insight_synthesizer_summary.md
  concurrency: 1
  retry:
    max_attempts: 3
    strategy: exponential
    cooldown_seconds: 15
  category: stage
