agent:
  id: gather.source_collector
  name: Source Collector
  stage: gather
  stage_label: Gather
  role: retriever
  description: Executes Source Collector responsibilities within the Gather stage.
  capabilities: []
  policies:
  - policy.evidence_threshold
  - policy.latency_budget
  - policy.research_depth
  - policy.seed.evidence_threshold
  - policy.seed.max_research_depth
  - policy.seed.max_total_time
  depends_on:
  - gather.coordinator
  - orchestration.supervisor
  - orchestration.observability_hub
  - orchestration.policy_enforcer
  - orchestration.audit_trail
  - orchestration.retry_manager
  - orchestration.quality_board
  inputs:
  - gather.input
  outputs:
  - gather.output
  prompts:
  - prompts/gather/source_collector_primary.md
  - prompts/gather/source_collector_fallback.md
  - prompts/gather/source_collector_summary.md
  concurrency: 1
  retry:
    max_attempts: 3
    strategy: exponential
    cooldown_seconds: 15
  category: stage
